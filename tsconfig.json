{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@backgroundTask/*": ["backgroundTask/*"],
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@services/*": ["services/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@context/*": ["context/*"],
      "@navigation/*": ["navigation/*"],
      "@typings/*": ["typings/*"],
      "@constants/*": ["constants/*"]
    }
  }
}
